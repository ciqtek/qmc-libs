{
  "name": "qmc-libs",
  "version": "0.0.1",
  "description": "Ciqtek Electron 前端工具库",
  "main": "dist/qmc-libs.js",
  "types":"types/index.d.ts",
  "scripts": {
    "build:components": "rollup -c ./build/rollup.config.components.js",
    "build:umd": "rollup -c ./build/rollup.config.prod.js",
    "build:dist": "rollup -c ./build/rollup.config.prod.js && rollup -c ./build/rollup.config.components.js",
    "build:doc":"jsdoc2md -t ./README.hbs ./src/**/index.js > README.md",
    "test": "npm run build:umd && jest"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ciqtek/qmc-libs.git"
  },
  "keywords": [
    "webworker",
    "echarts",
    "websocket"
  ],
  "files": [
    "dist",
    "types",
    "src"
  ],
  "author": "国仪量子（合肥）技术有限公司（xx@ciqtek.com）",
  "license": "null",
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@babel/plugin-transform-runtime": "^7.14.5",
    "@babel/preset-env": "^7.14.7",
    "@babel/runtime-corejs3": "^7.14.7",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^19.0.1",
    "@rollup/plugin-html": "^0.2.3",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.0.2",
    "babel-jest": "^27.0.6",
    "jest": "^27.0.6",
    "jsdoc-to-markdown": "^7.0.1",
    "rollup": "^2.53.1",
    "rollup-plugin-css-only": "^3.1.0",
    "rollup-plugin-livereload": "^2.0.5",
    "rollup-plugin-node-polyfills": "^0.2.1",
    "rollup-plugin-serve": "^1.1.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-web-worker-loader": "^1.6.1",
    "typescript": "^4.3.5"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "dom-to-image": "^2.6.0",
    "jszip": "^3.6.0"
  }
}
